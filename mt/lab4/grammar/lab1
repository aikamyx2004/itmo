grammar Lab1;

@package = ru.ainur.lab1;

startRule: e EOF;
e: t ePrime;

ePrime: PLUS t ePrime;
ePrime: MINUS t ePrime;
ePrime: ;

t: f tPrime;

tPrime: MULTIPLY f tPrime;
tPrime: DIVIDE f tPrime;
tPrime: ;

f: LPAREN e RPAREN;
f: MINUS f;
f: FUNC f;
f: NUMBER;

PLUS: "\\+";
MINUS: "-";
NUMBER: "\\d+";
MULTIPLY: "\\*";
DIVIDE: "/";
FUNC: "\\w+";
LPAREN: "\\(";
RPAREN: "\\)";
